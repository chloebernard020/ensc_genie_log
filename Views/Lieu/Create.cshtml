@model Lieu

@{
    ViewData["Title"] = "Create";
}

<h2>Ajout d'un parc</h2>
<hr />

<div class="mt-3">
    <a asp-action="Index" class="btn btn-outline-success mb-5">Retour Ã  la liste des parcs</a>

</div>
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger">

            </div>
            <div class="form-group">
                <label asp-for="Nom" class="control-label"></label>
                <input asp-for="Nom" class="form-control" />
                <span asp-validation-for="Nom" class="text-danger"></span>
            </div>
            <div class="form-group mt-2">
                <label asp-for="Type" class="control-label"></label>
                <select asp-for="Type" class="form-select" asp-items="ViewBag.ListeTypes"></select>
                <span asp-validation-for="Type" class="text-danger"></span>
            </div>
            <div class="form-group mt-4">
                <input type="submit" value="Ajouter un parc" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}